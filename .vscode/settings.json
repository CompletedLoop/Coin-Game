{
	"files.eol": "\n",
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"eslint.run": "onType",
	"eslint.format.enable": true,
	"typescript.tsdk": "node_modules/typescript/lib",
	"files.exclude": {
		"**/.git": false,
		"**/.svn": false,
		"**/.hg": false,
		"**/CVS": false,
		"**/.DS_Store": false,
		"**/Thumbs.db": false,
		".vscode": false,
		"**/include": false,
		"include": false,
		"node_modules": false,
		".eslintrc.json": false,
		".gitignore": false,
		".prettierrc": false,
		"aftman.toml": false,
		"Coin Game.rbxl": false,
		"Coin Game.rbxl.lock": false,
		"default.project.json": false,
		"tsconfig.json": false,
		"flamework.build": false,
		"package-lock.json": false,
		"README.md": false
	},
	"explorerExclude.backup": {}
}